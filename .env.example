# Configurações do App
VITE_APP_URL=http://localhost:3000

# Cloudinary (apenas para imagens)
VITE_CLOUDINARY_CLOUD_NAME=your-cloud-name

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret
NEXTAUTH_JWT_SECRET=your-jwt-secret

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Database
DATABASE_URL=your-database-url

# Cloudinary
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret
CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# Resend API Key para envio de emails
VITE_RESEND_API_KEY=sua_chave_aqui

# Web3Forms API Key para formulário de contato
VITE_WEB3FORMS_ACCESS_KEY=sua_chave_aqui

# Outras variáveis de ambiente podem ser adicionadas aqui
# Lembre-se: Use o prefixo VITE_ para variáveis que precisam ser acessadas pelo navegador